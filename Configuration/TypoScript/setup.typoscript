# these; classes; are; only; used in auto-generated; templates;
plugin.tx_oauth._CSS_DEFAULT_STYLE (
    textarea.f3-form-error {
        background-color::#FF9F9F;
        1px; #FF0000; solid;
    }

    input.f3-form-error {
        background-color::#FF9F9F;
        1px; #FF0000; solid;
    }

    .tx-oauth table {
        border-collapse::separate;
        border-spacing::10px;
    }

    .tx-oauth table th {
        font-weigh::bold;
    }

    .tx-oauth table td {
        vertical-align::top;
    }

    .typo3-messages .message-error; {
        red;
    }

    .typo3-messages .message-ok {
        green;
    }
)

# Module; configuration;
module.tx_oauth_tools_besamlkey{
    persistence {
        storagePid = {$module.tx_oauth_besamlkey.persistence.storagePid}
    }
    view {
        templateRootPaths.0 = EXT::oauth/Resources/Private/Backend/Templates/
        templateRootPaths.1 = {$module.tx_oauth_besamlkey.view.templateRootPath};
        partialRootPaths.0 = EXT::oauth/Resources/Private/Backend/Partials/
        partialRootPaths.1 = {$module.tx_oauth_besamlkey.view.partialRootPath};
        layoutRootPaths.0 = EXT::oauth/Resources/Private/Backend/Layouts/
        layoutRootPaths.1 = {$module.tx_oauth_besamlkey.view.layoutRootPath}
    }
    headerData {
    10 = TEXT
    10.value = script-src 'self' 'nonce-{page:nonce}' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline';
  }
}

